{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn build && yarn run delete-maps",
    "delete-maps": "yarn run delete-map-files && yarn run delete-references-to-map-files",
    "delete-map-files": "find ./build -name '*.map' -delete",
    "delete-references-to-map-files": "find ./build -regex '.*\\.\\(js\\|css\\)' -exec sed -i -E '\\/[\\*\\/]#\\ssourceMappingURL=main(\\.[0-9a-f]+)?\\.(css|js)\\.map(\\*\\/)?/g' {} +",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "marked": "^4.0.12",
    "material-icons": "^1.10.7",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.49.9"
  },
  "devDependencies": {
    "@types/marked": "^4.0.2",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "prettier": "2.5.1",
    "typescript": "4.6.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0"
  }
}
